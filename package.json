{
  "name": "advanced-data-extractor",
  "productName": "Advanced Data Extractor",
  "version": "1.0.0",
  "description": "Professional desktop web scraping tool with advanced data extraction capabilities",
  "main": "build/main/main.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "wait-on http://localhost:5173 && electron .",
    "build": "npm run build:renderer && npm run build:main && npm run build:preload",
    "build:renderer": "vite build",
    "build:main": "tsc src/main/main.ts --outDir build/main --target ES2020 --module ES2020 --moduleResolution node --allowSyntheticDefaultImports --esModuleInterop",
    "build:preload": "tsc src/preload/preload.ts --outDir build/preload --target ES2020 --module ES2020 --moduleResolution node --allowSyntheticDefaultImports --esModuleInterop && tsc src/preload/browserPreload.ts --outDir build/preload --target ES2020 --module ES2020 --moduleResolution node --allowSyntheticDefaultImports --esModuleInterop",
    "electron": "electron .",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "dist:mac": "npm run build && electron-builder --mac",
    "dist:win": "npm run build && electron-builder --win",
    "dist:linux": "npm run build && electron-builder --linux",
    "lint": "eslint ."
  },
  "keywords": [
    "electron",
    "web-scraping",
    "data-extraction",
    "chrome-extension",
    "automation"
  ],
  "author": "Advanced Data Solutions",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "better-sqlite3": "^8.7.0",
    "cheerio": "^1.0.0-rc.12",
    "electron": "^27.0.0",
    "lucide-react": "^0.344.0",
    "puppeteer-core": "^21.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/better-sqlite3": "^7.6.8",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "electron-builder": "^24.6.4",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "wait-on": "^7.2.0"
  },
  "build": {
    "extends": "./electron-builder.config.js"
  }
}